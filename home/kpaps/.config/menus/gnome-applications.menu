		<Menuname>System</Menuname>
		<Menuname>Universal Access</Menuname>
		<Menuname>Utilities</Menuname>
		<Menuname>Web Applications</Menuname>
		<Menuname>Other</Menuname>
		<Merge type="files"/>
	</Layout>
	<Menu>
		<Name>Development</Name>
		<Layout>
			<Merge type="menus"/>
			<Filename>code.desktop</Filename>
			<Filename>geany.desktop</Filename>
			<Filename>python3.11.desktop</Filename>
			<Filename>org.thonny.Thonny.desktop</Filename>
